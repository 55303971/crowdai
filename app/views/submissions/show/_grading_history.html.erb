<div class="row">
    <div class="col-md-10">
        <table class='table table-hover table-striped table-bordered leaderboard sortable'>
            <thead>
                <th>Grading Status</th>
                <th>Grading Factor</th>
                <th>Score</th>
                <% unless submission.challenge.secondary_sort_order == :not_used %>
                  <th>Score Secondary</th>
                <% end %>
                <th>Graded</th>
                <th>Message</th>
            </thead>
            <tbody>
                <% submission.submission_grades.each do |sg| %>
                <tr>
                    <td><%= sg.grading_status %></td>
                    <td><%= sg.grading_factor %></td>
                    <td><%= sg.score %></td>
                    <% unless submission.challenge.secondary_sort_order == :not_used %>
                      <td><%= sg.score_secondary %></td>
                    <% end %>
                    <td><%= display_dttm(sg.created_at) %></td>
                    <td><%= sg.grading_message %></td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>
</div>
