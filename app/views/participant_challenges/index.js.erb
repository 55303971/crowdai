$(".load-more").remove();
$("ul.list-challenge-participants").append("<%= j render partial: 'participant_challenges' %>");
$(".list-challenge-participants").last().after("<%= j render partial: 'shared/load_more', locals: { collection: @participant_challenges } %>");
$('html, body').animate({
   scrollTop: $(document).height()-$(window).height()},
   1400,
   "easeOutQuint"
);
