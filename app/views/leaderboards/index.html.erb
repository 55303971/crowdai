<content>
  <%= concept(Challenge::Cell::ChallengeMasthead, @challenge) %>
  <%= concept(Challenge::Cell::ChallengeSubnav, @challenge, tab: 'leaderboard') %>

  <div class="row" id='leaderboard'>
    <table class="leaderboard">
      <%= concept(Leaderboard::Cell::TableHead, @leaderboards, challenge: @challenge) %>
      <tbody>
        <%= render partial: 'leaderboards' %>
      </tbody>
    </table>
  </div>

  <%= render partial: 'leaderboard_footer' %>
  <%= render partial: 'shared/load_more', locals: { collection: @leaderboards } %>

  <div id='modal-anchor'></div>
</content>
