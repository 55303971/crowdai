<content>
  <%= concept(Challenge::Cell::ChallengeMasthead, @challenge) %>
  <%= concept(Challenge::Cell::ChallengeSubnav, @challenge, tab: 'discussion') %>

  <div class="row">
    <%= render partial: 'list_header', locals: { challenge: @challenge, topics: @topics } %>
    <ul class="list-topics">
      <%= render partial: 'topics' %>
    </ul>
    <%= render partial: 'shared/load_more', locals: { collection: @topics } %>
  </div>
</content>
