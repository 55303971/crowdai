<%= simple_form_for([@challenge,@dataset_file]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :seq %>
    <%= f.input :description %>
    <%= f.input :dataset_file, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
