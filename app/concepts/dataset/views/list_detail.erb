<li data-cell='Dataset::Cell::ListDetail'>
  <div class="details">
    <h5><%= link_to file_title, expiring_url %></h5>
    <p class="description"><%= file_description %></p>
    <% if policy(dataset_file).destroy? %>
      <p class='status active'><%= link_to 'delete', challenge_dataset_file_path(challenge,dataset_file), method: :delete, data: {confirm: 'Are you sure?'} %></p>
    <% end %>
  </div>
  <div class="stats">
    <div class="stat-fr">
      File Size<br>
      <span><%= file_size %></span>
    </div>
    <div class="stat-fr">
      File Format<br>
      <span><%= file_type %></span>
    </div>
  </div>
</li>
